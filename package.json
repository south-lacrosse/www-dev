{
 "name": "south-lacrosse-wwww",
 "version": "1.0.0",
 "description": "SEMLA WordPress Website",
 "author": "SEMLA",
 "license": "MIT",
 "repository": {
  "type": "git",
  "url": "https://github.com/south-lacrosse/www-dev.git"
 },
 "scripts": {
  "build:blocks": "wp-scripts build",
  "format:js": "wp-scripts format-js ./src",
  "lint:css": "wp-scripts lint-style",
  "lint:js": "wp-scripts lint-js",
  "start": "wp-scripts start",
  "packages-update": "wp-scripts packages-update",
  "build:blocks-map-dialog": "cross-env-shell inline-source --compress --root src/location/ src/location/map-dialog.html $npm_config_www/wp-content/plugins/semla/modal/map-dialog.html",
  "lint:site": "eslint src/*.js",
  "in-head-js": "uglifyjs src2/js/in-head.js -c -m -o src2/js/in-head.min.js",
  "lint:wp": "cross-env-shell eslint $npm_config_www/wp-content/plugins/semla/js/*.js",
  "js:cal": "cross-env-shell uglifyjs  $npm_config_www/wp-content/plugins/semla/js/cal.js -c -m -o $npm_config_www/wp-content/plugins/semla/js/cal.min.js",
  "js:colhover": "cross-env-shell uglifyjs $npm_config_www/wp-content/plugins/semla/js/colhover.js -c -m -o $npm_config_www/wp-content/plugins/semla/js/colhover.min.js",
  "js:embed": "cross-env-shell uglifyjs $npm_config_www/wp-content/plugins/semla/core/Rest/views/embed.js -c -m reserved=[c,i,s,t] -o $npm_config_www/wp-content/plugins/semla/core/Rest/views/embed.min.js",
  "js:map": "cross-env-shell uglifyjs $npm_config_www/wp-content/plugins/semla/js/map.js -c -m -o $npm_config_www/wp-content/plugins/semla/js/map.min.js",
  "js:toct": "cross-env-shell uglifyjs $npm_config_www/wp-content/plugins/semla/js/toct.js -c -m -o $npm_config_www/wp-content/plugins/semla/js/toct.min.js",
  "js:main": "cross-env-shell uglifyjs $npm_config_www/wp-content/themes/lax/main.js -c -m -o $npm_config_www/wp-content/themes/lax/main.min.js",
  "css:plugin": "cross-env-shell postcss \"$npm_config_www/wp-content/plugins/semla/css/!(*.min)*.css\" --no-map -d $npm_config_www/wp-content/plugins/semla/css --ext min.css -u postcss-csso autoprefixer",
  "css:theme": "cross-env-shell postcss $npm_config_www/wp-content/themes/lax/!(*.min)*.css --no-map -d $npm_config_www/wp-content/themes/lax --ext min.css -u postcss-csso autoprefixer",
  "clubs-map": "run-s js:map css:map",
  "build:wp": "run-s js:* css:*",
  "svg-optimize": "cross-env-shell svgo -f $npm_config_www/wp-content/themes/lax/img",
  "lint": "run-s lint:*",
  "build": "run-s build:*"
 },
 "devDependencies": {
  "@wordpress/icons": "^9.0.0",
  "@wordpress/scripts": "^23.1.0",
  "autoprefixer": "^10.4.7",
  "cross-env": "^7.0.3",
  "inline-source-cli": "^2.0.0",
  "npm-run-all": "^4.1.5",
  "postcss": "^8.4.14",
  "postcss-cli": "^9.1.0",
  "postcss-csso": "^6.0.0",
  "svgo": "^2.8.0",
  "uglify-js": "^3.15.5"
 }
}
