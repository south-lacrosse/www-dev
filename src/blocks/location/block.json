{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "semla/location",
	"version": "1.0.1",
	"title": "Location",
	"category": "widgets",
	"icon": "location",
	"description": "All information about a location - address, notes, map, directions",
	"attributes": {
		"address": {
			"type": "string",
			"source": "text",
			"selector": "p"
		},
		"mapperLinks": {
			"type": "boolean",
			"default": true
		}
	},
	"supports": {
		"html": false
	},
	"editorScript": "file:./index.js"
}
